<div>
  <p><%= @shift.start_time %> - <%= @shift.end_time %></p>
  <p>Dia: <%= @shift.day_of_week%></p>
  <p>turno: <%= @shift.turno %></p>
  <p>numero de empleados: <%= @shift.number_employees %></p>
  <ul>
    <% @shift.employees.each do |employee| %>
      <li><p>Empleado: <%= employee.name %></p></li>
    <% end %>
  </ul>
</div>
